<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
	id="WebApp_ID" version="3.1">
	<display-name>trabajo_final</display-name>
    <welcome-file-list>
        <welcome-file>EmpleadosController</welcome-file>
        <welcome-file>AreasController</welcome-file>
        <welcome-file>index.html</welcome-file>
        <welcome-file>index.jsp</welcome-file>
        <welcome-file>index.htm</welcome-file>
        <welcome-file>default.html</welcome-file>
        <welcome-file>default.jsp</welcome-file>
        <welcome-file>default.htm</welcome-file>
    </welcome-file-list>
	<servlet>
		<servlet-name>EmpleadosController</servlet-name>
		<servlet-class>Clases.EmpleadosController</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>AreasController</servlet-name>
		<servlet-class>Clases.AreasController</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>EmpleadosController</servlet-name>
		<url-pattern>/EmpleadosController</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>AreasController</servlet-name>
		<url-pattern>/AreasController</url-pattern>
	</servlet-mapping>
</web-app>